{
  "name": "Example",
  "description": "Example Graphite School",
  "private": false,
  "scripts": {
    "test": "npm run compile && mocha --compilers js:babel-core/register --recursive",
    "coverage": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- ./test --recursive",
    "lint": "eslint -f table ./client ./server ./imports || exit 1",
    "compile": "babel src --out-dir dist",
    "server:debug": "DEBUG=app,apollo-express,mongoose,mongoose-account,mongoose-account-password,mongoose-account-facebook node_modules/babel-cli/bin/babel-node.js ./server/main.js",
    "mongod": "(killall -9 mongod || true) && mkdir -p ./.build/data/log && mongod --fork --quiet --logpath ./.build/data/mongodb.log --dbpath ./.build/data --port 3001",
    "mongo:reset": "rm -R ./build/data",
    "start": "npm run mongod & npm run server:debug"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "dependencies": {
    "@graphite/apollo-express": "^0.2.16",
    "@graphite/decorators": "^0.2.17",
    "@graphite/mongoose": "^0.2.18",
    "apollo-client": "^0.10.1",
    "debug": "^2.6.1",
    "dyn-require": "0.0.3",
    "formsy-react": "^0.19.2",
    "graphql": "^0.9.5",
    "isomorphic-unfetch": "^1.0.0",
    "lodash": "^4.17.4",
    "next": "^2.2.0",
    "node-fetch": "^1.6.3",
    "react": "^15.4.2",
    "react-apollo": "^0.13.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.23.1",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.4.0",
    "babel-preset-latest": "^6.16.0",
    "chai": "^3.5.0",
    "debug": "^2.6.6",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "postcss-easy-import": "^2.0.0",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3"
  }
}
