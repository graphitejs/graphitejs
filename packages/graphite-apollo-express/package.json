{
  "name": "@graphite/apollo-express",
  "version": "0.3.1",
  "description": "Graphite Framework, apollo express",
  "main": "dist/Graphite.js",
  "scripts": {
    "test": "npm run compile && mocha --compilers js:babel-core/register --require babel-polyfill --recursive",
    "test:functional:riteway": "babel-node --root-mode upward ./test/functional",
    "coverage": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- ./test --recursive",
    "compile": "babel --root-mode upward src --out-dir dist",
    "compile:watch": "npm run compile -- -w --source-maps=inline",
    "prepublish": "npm run compile"
  },
  "author": "Walter Zalazar",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/node": "7.2.2",
    "@graphite/decorators": "0.3.1",
    "chai": "3.5.0",
    "debug": "2.3.3",
    "mocha": "3.2.0",
    "riteway": "4.0.1",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0"
  },
  "dependencies": {
    "@graphite/scalars": "^0.3.1",
    "apollo-server": "0.3.3",
    "body-parser": "1.15.2",
    "cors": "2.8.3",
    "express": "4.14.0",
    "graphql": "0.8.2",
    "graphql-tools": "0.8.3",
    "lodash": "4.17.2",
    "node-fetch": "1.7.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
