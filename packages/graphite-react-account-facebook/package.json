    "react-apollo": "0.13.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "chai": "3.5.0",
    "enzyme": "2.7.1",
    "jsdom": "9.12.0",
    "jsdom-global": "2.1.1",
    "mocha": "3.2.0",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.5.4",
    "react-test-renderer": "15.5.4",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0"
  },
  "publishConfig": {
    "access": "public"
