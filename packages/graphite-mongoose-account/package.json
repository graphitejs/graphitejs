{
  "name": "@graphite/mongoose-account",
  "version": "0.3.1",
  "description": "Graphite Framework, mongoose account",
  "main": "dist/account.js",
  "scripts": {
    "test": "npm run compile && mocha --compilers js:babel-core/register --require babel-polyfill --recursive",
    "coverage": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- ./test --recursive",
    "compile": "babel --root-mode upward src --out-dir dist",
    "compile:watch": "npm run compile -- -w --source-maps=inline",
    "prepublish": "npm run compile"
  },
  "author": "Walter Zalazar",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "chai": "3.5.0",
    "debug": "2.3.3",
    "mocha": "3.2.0",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0",
    "sinon-stub-promise": "4.0.0"
  },
  "dependencies": {
    "@graphite/decorators": "^0.3.1",
    "@graphite/mongoose": "^0.3.1",
    "bcrypt": "3.0.2",
    "jsonwebtoken": "7.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
